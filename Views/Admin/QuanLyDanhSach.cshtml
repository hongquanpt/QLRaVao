    List<AccountRole> data = HttpContextAccessor.HttpContext.Session.GetJson<List<AccountRole>>("QuyenTK") ?? new List<AccountRole>();
    List<DSGT> ds = HttpContextAccessor.HttpContext.Session.GetJson<List<DSGT>>("DS") ?? new List<DSGT>();
    List<Giayto> gt = HttpContextAccessor.HttpContext.Session.GetJson<List<Giayto>>("GT") ?? new List<Giayto>();
    List<Quannhan> hv = HttpContextAccessor.HttpContext.Session.GetJson<List<Quannhan>>("HV") ?? new List<Quannhan>();
